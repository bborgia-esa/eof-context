{
  "@context": {
    "eof": "https://raw.githubusercontent.com/bborgia-esa/eof-context/main/ontology.ttl#",
    "dcat": "http://www.w3.org/ns/dcat#",
    "dct": "http://purl.org/dc/terms/",
    "schema": "https://schema.org/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",

    "id": "@id",
    "type": "@type",

    "name": {
      "@id": "schema:name",
      "@type": "xsd:string"
    },
    "description": {
      "@id": "schema:description",
      "@type": "xsd:string"
    },
    "version": {
      "@id": "eof:version",
      "@type": "xsd:string"
    },
    "endpoint": {
      "@id": "eof:endpoint",
      "@type": "xsd:anyURI"
    },
    "documentation": {
      "@id": "eof:documentation",
      "@type": "xsd:anyURI"
    },
    "tags": {
      "@id": "eof:tags",
      "@container": "@set"
    },
    "serviceType": {
      "@id": "eof:serviceType",
      "@type": "xsd:string"
    },
    "serviceCategory": {
      "@id": "eof:serviceCategory",
      "@type": "xsd:string"
    },
    "organization": {
      "@id": "eof:organization",
      "@type": "@id"
    },
    "hasDataOffer": {
      "@id": "eof:hasDataOffer",
      "@type": "@id"
    },
    "hasInterface": {
      "@id": "eof:hasInterface",
      "@type": "@id"
    },
    "document": {
      "@id": "eof:document",
      "@type": "xsd:anyURI"
    },
    "timeAvailability": {
      "@id": "eof:timeAvailability",
      "@type": "xsd:string"
    },
    "listsProgram": {
      "@id": "eof:listsProgram",
      "@type": "@id"
    },
    "includesDataset": {
      "@id": "eof:includesDataset",
      "@type": "@id"
    },
    "dependsOnDataset": {
      "@id": "eof:dependsOnDataset",
      "@type": "@id"
    },
    "retrievedFrom": {
      "@id": "eof:retrievedFrom",
      "@type": "@id"
    },
    "created": {
      "@id": "dct:created",
      "@type": "xsd:dateTime"
    },
    "modified": {
      "@id": "dct:modified",
      "@type": "xsd:dateTime"
    }
  }
}
